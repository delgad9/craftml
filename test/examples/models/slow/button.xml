<craft>

    <craft name="sun-layout" src="../fast/sun-layout.xml"/>

    <stack layout="centerXYZ()" cut=".cut">
        <group class="cut">
        <sun-layout radius="30">
            <repeat n="8">
                <cylinder/>
            </repeat>
        </sun-layout>
        </group>
        <cylinder transform="scale(10,10,1)"></cylinder>
