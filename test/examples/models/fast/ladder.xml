<craft name="ladder">
    <parameter name="width" type="int" default="20"/>
    <parameter name="height" type="int" default="100"/>

    <row>
        <cube size="5 {{height}} 5"></cube>
        <col spacing="10">
            <repeat n="{{(height / 10) * 0.7}}">
                <cylinder radius="1.5" height="{{width}}" transform="rotateY(90)"></cylinder>
            </repeat>
        </col>
        <cube size="5 {{height}} 5"></cube>
    </row>

</craft>
