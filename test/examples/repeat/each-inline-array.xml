<craft>
    <repeat each="x" in="{{[1,2,3]}}">
        <cube size="{{x}} 10 10"></cube>
    </repeat>
    <script>
        expect(this.parent).have.children(['cube','cube','cube','script'])
        expect(this.parent).have.solid('cube').at([0]).have.size(1,10,10)
        expect(this.parent).have.solid('cube').at([1]).have.size(2,10,10)
        expect(this.parent).have.solid('cube').at([2]).have.size(3,10,10)
    </script>
</craft>
