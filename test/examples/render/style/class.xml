<craft>
    <style>
        .foo {
            something: black;
        }
        .bar {
            something: green;
        }
    </style>
    <cube class="foo"></cube>
    <script>
        expect(this.prev).to.have
            .style('something', 'black')
    </script>
    <cube class="bar"></cube>
    <script>
        expect(this.prev).to.have
            .style('something', 'green')
    </script>
</craft>
