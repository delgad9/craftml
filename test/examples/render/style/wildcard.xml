<craft>

    <style>
        * {
            something: good;
        }
    </style>
    <g>
        <g id="inside">
            <cube></cube>
        </g>
        <sphere></sphere>
    </g>

    <script>
        $('cube').should.have.style('something', 'good')
        $('sphere').should.have.style('something', 'good')
        $('#inside').should.have.style('something', 'good')
    </script>

</craft>
