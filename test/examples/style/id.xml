<craft>
    <style>
        #foo {
            something: black;
        }
        #bar {
            something: green;
        }
    </style>
    <cube id="foo"></cube>
    <script>
        expect(this.prev).to.have
            .style('something', 'black')
    </script>
    <cube id="bar"></cube>
    <script>
        expect(this.prev).to.have
            .style('something', 'green')
    </script>
</craft>
