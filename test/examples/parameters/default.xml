<craft>

    <parameter name="one" type="int" default="1"/>
    <parameter name="two" type="int" default="2"/>
    <parameter name="three" type="int" default="3"/>
    <parameter name="text" type="string" default="foo"/>

    <script>
        function main(params, scope){
            expect(params).to.have.property('one').and.eql(1)
            expect(params).to.have.property('two').and.eql(2)
            expect(params).to.have.property('three').and.eql(3)
            expect(params).to.have.property('text').and.equal('foo')
        }
    </script>

</craft>
