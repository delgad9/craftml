<craft>
    <cube t="setX(100)"></cube>
    <script>
        expect(this.prev).to.have.location(100,0,0)
    </script>

    <cube t="setY(100)"></cube>
    <script>
        expect(this.prev).to.have.location(0,100,0)
    </script>

    <cube t="setZ(100)"></cube>
    <script>
        expect(this.prev).to.have.location(0,0,100)
    </script>

</craft>
