<craft name="rectangle">
    <parameter name="width" default="10" type="float"/>
    <parameter name="height" default="10" type="int"/>
    <script type="text/openjscad">
        this.add2D(new CAG.rectangle({radius: [params.width/2, params.height/2], center: [params.width/2, params.height/2]}))
    </script>
</craft>
