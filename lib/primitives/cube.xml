<craft name="cube">
    <parameter name="xsize" default="5" type="int"/>
    <parameter name="ysize" default="5" type="int"/>
    <parameter name="zsize" default="5" type="int"/>
    <script type="text/craftml">
        function main(params){
            return cube([params.xsize,params.ysize,params.zsize])
        }
    </script>
</craft>